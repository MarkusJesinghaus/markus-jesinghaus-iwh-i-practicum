doctype html
html
  head
    title= title
    link(rel="stylesheet", href="/css/style.css")
  body
    h1 Custom Object Records
    a(href="/update-cobj") Add to this table
    br
    br
    if records.length
      table(border="1")
        thead
          tr
            th Name
            th Category
            th Type
        tbody
          each record in records
            tr
              td= record.properties.game_name
              td= record.properties.game_category
              td= record.properties.game_type
    else
      p No records found.
